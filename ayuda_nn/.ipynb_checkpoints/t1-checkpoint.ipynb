    "def nn(m1,m2,w1,w2,b):\n",
    "    z=w1*m1+w2*m2+b\n",
    "    return sigmoid(z)\n",
    "def sigmoid(x):\n",
    "    return (1/(1+numpy.exp(-x)))"
   ]
